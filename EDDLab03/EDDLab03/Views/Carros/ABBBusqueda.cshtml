@model List<Carro>
@{
	ViewData["Title"] = "Resultados de busqueda ABB";
}
<body style="background-color: moccasin;">

	<h1>@ViewData["Title"] </h1>
	@if (Model != null)
	{
					<table class="table table-hover">
						<thead>
							<tr>
								<td>ID</td>
								<td>Email</td>
								<td>Propietario</td>
								<td>Color</td>
								<td>Marca</td>
								<td>Serie</td>
							</tr>
						</thead>
			@foreach (var carro in Model)
			{
									<tr>
										<td>@carro.id</td>
										<td>@carro.email</td>
										<td>@carro.propietario</td>
										<td>@carro.color</td>
										<td>@carro.marca</td>
										<td>@carro.serie</td>
									</tr>
			}
					</table>
	}
</body>