@model List<Carro>
@{
    ViewData["Title"] = "Carga de CSV";
}

<h1>@ViewData["Title"] </h1>

@using (Html.BeginForm("CargaCSV", "Carros", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
	<label>Subir Archivo</label>
	<br />
	<input type="file" name="archivo" />
	<input type="submit" , value="Enviar" />

	@if (Model != null)
	{
		<table class="table table-hover">
			<thead>
				<tr>
					<td>ID</td>
					<td>Email</td>
					<td>Propietario</td>
					<td>Color</td>
					<td>Marca</td>
					<td>Serie</td>
						</tr>
					</thead>
			@foreach (var carro in Model)
			{
				<tr>
					<td>@carro.id</td>
					<td>@carro.email</td>
					<td>@carro.propietario</td>
					<td>@carro.color</td>
					<td>@carro.marca</td>
					<td>@carro.serie</td>
				</tr>
			}
				</table>
	}
}